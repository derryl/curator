// Secrets.xcconfig.example â€” Copy to Secrets.xcconfig and fill in real values.
// Secrets.xcconfig is gitignored and will not be committed.

// Trakt OAuth app credentials (required for all builds)
TRAKT_CLIENT_ID = your_trakt_client_id_here
TRAKT_CLIENT_SECRET = your_trakt_client_secret_here

// Debug-only: auto-seed Overseerr connection on fresh install
DEBUG_OVERSEERR_CONNECTION_TYPE = http
DEBUG_OVERSEERR_ADDRESS = your_overseerr_ip_here
DEBUG_OVERSEERR_PORT = 5055
DEBUG_OVERSEERR_API_KEY = your_base64_api_key_here

// Debug-only: auto-seed Trakt tokens on fresh install
DEBUG_TRAKT_ACCESS_TOKEN = your_access_token_here
DEBUG_TRAKT_REFRESH_TOKEN = your_refresh_token_here

// CouchMoney Trakt account
COUCHMONEY_USERNAME = your_trakt_username_here
